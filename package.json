{
  "author": "Joaquin Briceno <joaquinbriceno1@gmail.com>",
  "name": "node-phraseapp",
  "version": "0.0.1",
  "description": "Wrapper for Phraseapp REST API",
  "keywords": [
    "phraseapp",
    "api",
    "rest",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Joaquin6/node-phraseapp"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6",
    "yarn": ">=1.9.0"
  },
  "scripts": {
    "build": "babel src -d lib",
    "dev": "yarn dev:watch:server",
    "dev:watch:server": "nodemon -e js,json,md --watch src --exec \"yarn start:app\" | bunyan",
    "lint": "eslint src/; eslint test/",
    "lint:fix": "eslint --fix src/; eslint --fix test/",
    "lint:staged": "git diff --cached --name-only -- src/ test/ | egrep \"\\\\.js$\" | xargs -L1 eslint",
    "postinstall": "yarn build",
    "start": "yarn start:app",
    "start:app": "node -r babel-register src/index.js",
    "test": "mocha test/**/*.spec.js"
  },
  "precommit": [
    "lint:staged"
  ],
  "dependencies": {
    "fs-extra": "7.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-mocha": "5.2.0",
    "eslint-plugin-standard": "4.0.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.4",
    "pre-commit": "1.2.2"
  }
}
